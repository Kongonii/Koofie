<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.multicampus.kb03.weddingBuddy.repository.mybatis.mapper.ChatMapper">

	
	<select id="select" parameterType="com.multicampus.kb03.weddingBuddy.dto.ChattingDto"
			resultType="java.lang.Integer">
		SELECT c.CHATTING_ID
		FROM CHATTING c
		INNER JOIN USER_ACCOUNT u ON c.user_id = u.user_id
		WHERE c.user_id = #{user_id}
	</select>

</mapper>
